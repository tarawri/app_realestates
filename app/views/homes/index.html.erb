<h1>Homes#index</h1>
<table class="table table-striped table-dark">
<tr> 
<th>id</th>
<th>title</th>
<th>details</th>
</tr>
<% @realestates.each do |real|%>

<tr>
<td><%= real.id%></td>
<td><%= real.title%></td>
<td> <%= real.details%></td>
<td> <%= link_to "show", realestate_path(real)%></td>

</tr>
<%end%>

</table>
